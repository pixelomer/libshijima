			</Animation>
		</Action>


		<!-- Dragging -->

		<Action Name="Pinched" Type="Embedded" Class="com.group_finity.mascot.action.Dragged">
			<Animation Condition="#{FootX &lt; mascot.environment.cursor.x-50}">
				<Pose Image="/shime9.png" ImageAnchor="110,180" Velocity="0,0" Duration="5" />
			</Animation>
			<Animation Condition="#{FootX &lt; mascot.environment.cursor.x-30}">
				<Pose Image="/shime7.png" ImageAnchor="110,180" Velocity="0,0" Duration="5" />
			</Animation>
			<Animation Condition="#{FootX &lt; mascot.environment.cursor.x+30}">
				<Pose Image="/shime69.png" ImageAnchor="110,180" Velocity="0,0" Duration="80" />
				<Pose Image="/shime68.png" ImageAnchor="110,180" Velocity="0,0" Duration="3" />
				<Pose Image="/shime69.png" ImageAnchor="110,180" Velocity="0,0" Duration="80" />
				<Pose Image="/shime68.png" ImageAnchor="110,180" Velocity="0,0" Duration="3" />
