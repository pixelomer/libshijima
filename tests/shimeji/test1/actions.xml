<?xml version="1.0" encoding="UTF-8" ?>
<Mascot xmlns="http://www.group-finity.com/Mascot" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.group-finity.com/Mascot Mascot.xsd">

	<ActionList>
		<Action Name="Look" Type="Embedded" Class="com.group_finity.mascot.action.Look" />
		<Action Name="Offset" Type="Embedded" Class="com.group_finity.mascot.action.Offset" />

		<!-- Standing -->

		<Action Name="Stand" Type="Stay" BorderType="Floor">
			<Animation>
				<Pose Image="/shime1.png" ImageAnchor="128,192" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>

		<Action Name="Walk" Type="Move" BorderType="Floor">
			<Animation>
				<Pose Image="/shime2.png" ImageAnchor="128,192" Velocity="-2,0" Duration="6" />
				<Pose Image="/shime3.png" ImageAnchor="128,192" Velocity="-2,0" Duration="6" />
				<Pose Image="/shime48.png" ImageAnchor="128,192" Velocity="-2,0" Duration="6" />
				<Pose Image="/shime47.png" ImageAnchor="128,192" Velocity="-2,0" Duration="6" />
			</Animation>
		</Action>

		<Action Name="Run" Type="Move" BorderType="Floor">
			<Animation>
				<Pose Image="/shime49.png" ImageAnchor="128,192" Velocity="-6,0" Duration="4" />
				<Pose Image="/shime50.png" ImageAnchor="128,192" Velocity="-6,0" Duration="4" />
				<Pose Image="/shime51.png" ImageAnchor="128,192" Velocity="-6,0" Duration="4" />
				<Pose Image="/shime52.png" ImageAnchor="128,192" Velocity="-6,0" Duration="4" />
			</Animation>
		</Action>

		<Action Name="Dash" Type="Move" BorderType="Floor">
			<Animation>
				<Pose Image="/shime49.png" ImageAnchor="128,192" Velocity="-8,0" Duration="4" />
				<Pose Image="/shime50.png" ImageAnchor="128,192" Velocity="-8,0" Duration="4" />
				<Pose Image="/shime51.png" ImageAnchor="128,192" Velocity="-8,0" Duration="4" />
				<Pose Image="/shime52.png" ImageAnchor="128,192" Velocity="-8,0" Duration="4" />
			</Animation>
		</Action>

		<!-- Sitting -->

		<Action Name="Sit" Type="Stay" BorderType="Floor">
			<Animation>
				<Pose Image="/shime11.png" ImageAnchor="128,188" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>
		<Action Name="SitAndLookUp" Type="Stay" BorderType="Floor">
			<Animation>
				<Pose Image="/shime26.png" ImageAnchor="128,188" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>

		<Action Name="SitAndLookAtMouse" Type="Stay" BorderType="Floor">
			<Animation Condition="#{mascot.environment.cursor.y &lt; mascot.environment.screen.height/2}">
				<Pose Image="/shime26.png" ImageAnchor="128,188" Velocity="0,0" Duration="250" />
			</Animation>
			<Animation>
				<Pose Image="/shime11.png" ImageAnchor="128,188" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>

		<Action Name="SitAndSpinHeadAction" Type="Animate" BorderType="Floor">
			<Animation>
				<Pose Image="/shime15.png" ImageAnchor="128,171" Velocity="0,0" Duration="4" />
				<Pose Image="/shime16.png" ImageAnchor="128,171" Velocity="0,0" Duration="4" />
				<Pose Image="/shime17.png" ImageAnchor="128,171" Velocity="0,0" Duration="3" />
				<Pose Image="/shime104.png" ImageAnchor="128,171" Velocity="0,0" Duration="3" />
				<Pose Image="/shime105.png" ImageAnchor="128,171" Velocity="0,0" Duration="4" />
				<Pose Image="/shime106.png" ImageAnchor="128,171" Velocity="0,0" Duration="4" />
				<Pose Image="/shime107.png" ImageAnchor="128,171" Velocity="0,0" Duration="4" />
				<Pose Image="/shime108.png" ImageAnchor="128,171" Velocity="0,0" Duration="5" />
				<Pose Image="/shime109.png" ImageAnchor="128,171" Velocity="0,0" Duration="5" />
				<Pose Image="/shime110.png" ImageAnchor="128,171" Velocity="0,0" Duration="5" />
				<Pose Image="/shime111.png" ImageAnchor="128,171" Velocity="0,0" Duration="5" />
				<Pose Image="/shime112.png" ImageAnchor="128,171" Velocity="0,0" Duration="50" />
			</Animation>
		</Action>

		<Action Name="SitWithLegsUp" Type="Stay" BorderType="Floor">
			<Animation>
				<Pose Image="/shime30.png" ImageAnchor="128,192" Velocity="0,0" Duration="5" />
			</Animation>
		</Action>

		<Action Name="SitWithLegsDown" Type="Stay" BorderType="Floor">
			<Animation>
				<Pose Image="/shime31.png" ImageAnchor="128,192" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>

		<Action Name="SitAndDangleLegs" Type="Stay" BorderType="Floor">
			<Animation>
				<Pose Image="/shime30.png" ImageAnchor="128,192" Velocity="0,0" Duration="3" />
				<Pose Image="/shime31.png" ImageAnchor="128,192" Velocity="0,0" Duration="3" />
				<Pose Image="/shime32.png" ImageAnchor="128,192" Velocity="0,0" Duration="3" />
				<Pose Image="/shime31.png" ImageAnchor="128,192" Velocity="0,0" Duration="3" />
			</Animation>
		</Action>

		<!-- Laying -->

		<Action Name="Sprawl" Type="Stay" BorderType="Floor">
			<Animation>
				<Pose Image="/shime21.png" ImageAnchor="128,192" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>

		<Action Name="Creep" Type="Move" BorderType="Floor">
			<Animation>
				<Pose Image="/shime20.png" ImageAnchor="128,192" Velocity="-2,0" Duration="4" />
				<Pose Image="/shime21.png" ImageAnchor="128,192" Velocity="-2,0" Duration="4" />
				<Pose Image="/shime20.png" ImageAnchor="128,192" Velocity="-2,0" Duration="4" />
				<Pose Image="/shime94.png" ImageAnchor="128,192" Velocity="-2,0" Duration="4" />
			</Animation>
		</Action>

		<!-- Ceiling -->

		<Action Name="GrabCeiling" Type="Stay" BorderType="Ceiling">
			<Animation>
				<Pose Image="/shime23.png" ImageAnchor="64,28" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>
		
		<Action Name="ClimbCeiling" Type="Move" BorderType="Ceiling">
			<Animation>
				<Pose Image="/shime25.png" ImageAnchor="64,28" Velocity="0,0" Duration="16" />
				<Pose Image="/shime25.png" ImageAnchor="64,28" Velocity="-1,0" Duration="4" />
				<Pose Image="/shime23.png" ImageAnchor="64,28" Velocity="-1,0" Duration="4" />
				<Pose Image="/shime24.png" ImageAnchor="64,28" Velocity="-1,0" Duration="4" />
				<Pose Image="/shime24.png" ImageAnchor="64,28" Velocity="0,0" Duration="16" />
				<Pose Image="/shime24.png" ImageAnchor="64,28" Velocity="-2,0" Duration="4" />
				<Pose Image="/shime23.png" ImageAnchor="64,28" Velocity="-2,0" Duration="4" />
				<Pose Image="/shime25.png" ImageAnchor="64,28" Velocity="-2,0" Duration="4" />
			</Animation>
		</Action>

		<!-- Wall -->

		<Action Name="GrabWall" Type="Stay" BorderType="Wall">
			<Animation>
				<Pose Image="/shime13.png" ImageAnchor="28,128" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>
		<Action Name="ClimbWall" Type="Move" BorderType="Wall">
			<Animation Condition="#{TargetY &lt; mascot.anchor.y}">
				<Pose Image="/shime14.png" ImageAnchor="28,128" Velocity="0,0" Duration="16" />
				<Pose Image="/shime14.png" ImageAnchor="28,128" Velocity="0,-1" Duration="4" />
				<Pose Image="/shime12.png" ImageAnchor="28,128" Velocity="0,-1" Duration="4" />
				<Pose Image="/shime13.png" ImageAnchor="28,128" Velocity="0,-1" Duration="4" />
				<Pose Image="/shime13.png" ImageAnchor="28,128" Velocity="0,0" Duration="16" />
				<Pose Image="/shime13.png" ImageAnchor="28,128" Velocity="0,-2" Duration="4" />
				<Pose Image="/shime12.png" ImageAnchor="28,128" Velocity="0,-2" Duration="4" />
				<Pose Image="/shime14.png" ImageAnchor="28,128" Velocity="0,-2" Duration="4" />
			</Animation>

			<Animation Condition="#{TargetY &gt;= mascot.anchor.y}">
				<Pose Image="/shime14.png" ImageAnchor="28,128" Velocity="0,0" Duration="16" />
				<Pose Image="/shime14.png" ImageAnchor="28,128" Velocity="0,2" Duration="4" />
				<Pose Image="/shime12.png" ImageAnchor="28,128" Velocity="0,2" Duration="4" />
				<Pose Image="/shime13.png" ImageAnchor="28,128" Velocity="0,2" Duration="4" />
				<Pose Image="/shime13.png" ImageAnchor="28,128" Velocity="0,0" Duration="16" />
				<Pose Image="/shime13.png" ImageAnchor="28,128" Velocity="0,1" Duration="4" />
				<Pose Image="/shime12.png" ImageAnchor="28,128" Velocity="0,1" Duration="4" />
				<Pose Image="/shime14.png" ImageAnchor="28,128" Velocity="0,1" Duration="4" />
			</Animation>
		</Action>

		<!-- IE -->

		<Action Name="FallWithIe" Type="Embedded" Class="com.group_finity.mascot.action.FallWithIE"
			 IeOffsetX="0" IeOffsetY="-64">
			<Animation>
				<Pose Image="/shime36.png" ImageAnchor="64,150" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>
		<Action Name="WalkWithIe" BorderType="Floor" Type="Embedded" Class="com.group_finity.mascot.action.WalkWithIE"
			 IeOffsetX="0" IeOffsetY="-64">
			<Animation>
				<Pose Image="/shime34.png" ImageAnchor="64,150" Velocity="-4,0" Duration="4" />
				<Pose Image="/shime35.png" ImageAnchor="64,150" Velocity="-4,0" Duration="4" />
				<Pose Image="/shime37.png" ImageAnchor="64,150" Velocity="-4,0" Duration="4" />
				<Pose Image="/shime36.png" ImageAnchor="64,150" Velocity="-4,0" Duration="4" />
			</Animation>
		</Action>
		<Action Name="RunWithIe" BorderType="Floor" Type="Embedded" Class="com.group_finity.mascot.action.WalkWithIE"
			 IeOffsetX="0" IeOffsetY="-64">
			<Animation>
				<Pose Image="/shime34.png" ImageAnchor="64,150" Velocity="-8,0" Duration="2" />
				<Pose Image="/shime35.png" ImageAnchor="64,150" Velocity="-8,0" Duration="2" />
				<Pose Image="/shime37.png" ImageAnchor="64,150" Velocity="-8,0" Duration="2" />
				<Pose Image="/shime36.png" ImageAnchor="64,150" Velocity="-8,0" Duration="2" />
			</Animation>
		</Action>
		<Action Name="ThrowIe" BorderType="Floor" Type="Embedded" Class="com.group_finity.mascot.action.ThrowIE"
			InitialVX="32" InitialVY="-10" Gravity="0.5">
			<Animation>
				<Pose Image="/shime38.png" ImageAnchor="64,150" Velocity="0,0" Duration="40" />
			</Animation>
		</Action>

		<!-- Falling -->

		<Action Name="Jumping" Type="Embedded" Class="com.group_finity.mascot.action.Jump"
			VelocityParam="16">
			<Animation>
				<Pose Image="/shime22.png" ImageAnchor="128,192" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>

		<Action Name="SlowJumping" Type="Embedded" Class="com.group_finity.mascot.action.Jump"
			VelocityParam="12">
			<Animation>
				<Pose Image="/shime22.png" ImageAnchor="128,192" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>

		<Action Name="Falling" Type="Embedded" Class="com.group_finity.mascot.action.Fall"
			RegistanceX="0.05" RegistanceY="0.1" Gravity="#{(Math.random() > 0.5) ? 2 : 3}">
