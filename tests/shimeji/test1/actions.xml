				<Pose Image="/shime4.png" ImageAnchor="128,192" Velocity="0,0" Duration="3" />
				<Pose Image="/shime53.png" ImageAnchor="128,192" Velocity="0,0" Duration="3" />
				<Pose Image="/shime4.png" ImageAnchor="128,192" Velocity="0,0" Duration="3" />
				<Pose Image="/shime54.png" ImageAnchor="128,192" Velocity="0,0" Duration="3" />
			</Animation>
		</Action>

		<Action Name="Bouncing" Type="Animate" BorderType="Floor">
			<Animation>
				<Pose Image="/shime55.png" ImageAnchor="128,192" Velocity="0,0" Duration="4" />
				<Pose Image="/shime56.png" ImageAnchor="128,192" Velocity="0,0" Duration="6" />
				<Pose Image="/shime57.png" ImageAnchor="128,192" Velocity="0,0" Duration="4" />
				<Pose Image="/shime58.png" ImageAnchor="128,192" Velocity="0,0" Duration="4" />
				<Pose Image="/shime59.png" ImageAnchor="128,192" Velocity="0,0" Duration="4" />
				<Pose Image="/shime60.png" ImageAnchor="128,192" Velocity="0,0" Duration="4" />
