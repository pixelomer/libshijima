			</Animation>
		</Action>

		<Action Name="Tripping" Type="Animate" BorderType="Floor">
			<Animation>
				<Pose Image="/shime19.png" ImageAnchor="128,192" Velocity="0,0" Duration="3" />
				<Pose Image="/shime18.png" ImageAnchor="128,192" Velocity="0,0" Duration="3" />
