				<Pose Image="/shime68.png" ImageAnchor="110,180" Velocity="0,0" Duration="3" />
			</Animation>
			<Animation Condition="#{FootX &lt; mascot.environment.cursor.x+50}">
				<Pose Image="/shime8.png" ImageAnchor="110,180" Velocity="0,0" Duration="5" />
			</Animation>
			<Animation Condition="#{FootX &gt;= mascot.environment.cursor.x+30}">
				<Pose Image="/shime10.png" ImageAnchor="110,180" Velocity="0,0" Duration="5" />
			</Animation>
